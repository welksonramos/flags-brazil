{
  "name": "flags-brazil",
  "version": "0.1.2",
  "description": "Coleção de bandeiras dos Estados Federativos do Brasil feitas somente com CSS",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "stylus src/ --out dist/",
    "server": "browser-sync start --files \"*.html, dist/**/*.css\" --server",
    "watch:css": "onchange \"src/**/*.styl\" -- npm run build:css",
    "dev": "parallelshell \"npm run watch:css\" \"npm run server\"",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/welksonramos/flags-brazil.git"
  },
  "keywords": [
    "flags",
    "brasil",
    "brazil",
    "bandeiras"
  ],
  "author": "Welkson Ramos <welksonr@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/welksonramos/flags-brazil/issues"
  },
  "homepage": "https://github.com/welksonramos/flags-brazil#readme",
  "devDependencies": {
    "all-contributors-cli": "^5.10.2",
    "browser-sync": "^2.11.1",
    "onchange": "^2.1.2",
    "parallelshell": "^3.0.2",
    "stylus": "^0.54.2"
  }
}
